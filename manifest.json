{
    "name": "ChatGPT + GMail",
    "version": "1.1.1",
    "manifest_version":3,
    "author": "abhijeet singh",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts":[
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["gmail-script.js"]
        },
        {
            "matches": ["https://chat.openai.com/*"],
            "js": ["gpt-script.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions":[
        "https://mail.google.com/*",
        "https://chat.openai.com/*"
    ]
}